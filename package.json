{
    "name": "@yankeeinlondon/package-json",
    "version": "1.0.0-beta.1",
    "description": "identifies, types, loads, and pre-fills various package files",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "test": "vitest",
        "build": "tsdown src/index.ts --format=esm,cjs --dts",
        "lint": "eslint src --fix",
        "watch": "tsdown src/index.ts --format=esm,cjs --dts --watch",
        "release": "bumpp"
    },
    "keywords": [
        "parse",
        "package.json",
        "initialize"
    ],
    "author": "Ken Snyder <ken@ken.net>",
    "license": "MIT",
    "packageManager": "pnpm@10.10.0",
    "devDependencies": {
        "@antfu/eslint-config": "^4.13.2",
        "@type-challenges/utils": "^0.1.1",
        "@types/node": "^22.15.24",
        "bumpp": "^10.1.1",
        "eslint": "^9.27.0",
        "eslint-plugin-format": "^1.0.1",
        "husky": "^9.1.7",
        "jiti": "^2.4.2",
        "npm-run-all": "^4.1.5",
        "tsdown": "^0.12.4",
        "typescript": "^5.8.3",
        "vitest": "^3.1.4"
    },
    "peerDependencies": {
        "@yankeeinlondon/kind-error": ">=1.3.2",
        "inferred-types": ">=0.55.23",
        "simple-git": ">=3.27.0"
    }
}
